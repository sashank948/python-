def solve(s):
    return ' '.join(map(str.capitalize, s.split(' ')))
    '''s1=[]
    s=s.split()
    print(s)
    for i in s:
        s1.append(i.capitalize())
    return ' '.join(s1)'''

if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    s = input()

    result = solve(s)

    fptr.write(result + '\n')

    fptr.close()
